extends layouts/default

block vars
	-
		const page = {
			title: 'index.html'
		};
		menu_item_1_additional_classes="menu__list-item--active"




block main
	.breadcrumbs
		.container 
			ul.breadcrumbs__list 
				li.breadcrumbs__item
					a.breadcrumbs__link Home
				li.breadcrumbs__item
					a.breadcrumbs__link About
	section.about 
		.about__prev
			.container 
				h2.about__prev-title About Us
		.about__content 
			.container
				h3.about__content-title Balance in the simple through an informed marriage of design, functional and craft
				.about__text
					p Vexillologist vape microdosing freegan pork belly deep v direct trade cray single-origin coffee street art. Viral shaman mustache master cleanse, pour-over affogato poutine copper mug marfa fanny pack normcore. Lo-fi pop-up banjo portland, echo park hammock chillwave literally post-ironic ramps subway tile kitsch. Shaman tumblr fixie echo park street art. Enamel pin vaporware wayfarers, organic succulents hella sustainable cardigan.
					p Sustainable hot chicken skateboard, dreamcatcher meggings actually squid. Slow-carb everyday carry +1 art party microdosing, put a bird on it brooklyn tilde distillery pork belly single-origin coffee tumblr quinoa copper mug. Affogato cold-pressed unicorn, viral intelligentsia leggings raw denim kombucha tote bag messenger bag chambray vape coloring book scenester. Activated charcoal wayfarers church-key tote bag synth brunch. Retro cornhole
		.about__popup
			a.about__popup-play(data-fancybox, href="https://youtu.be/ox43Z8iqwks")
				img.about__popup-imgbtn(src="images/play_button.svg", alt="")
	section.inspiration
		.container-fluid 
			.inspiration__inner 
				img.inspiration__images(src="images/inspiration-decor.png", alt="")
				.inspiration__content 
					h3.inspiration__title Our Inspiration in details
					p.inspiration__text Semiotics fixie four dollar toast, next level woke scenester direct trade photo booth helvetica jean shorts. Fanny pack church-key cornhole, banh mi thundercats gochujang keytar.
					ul.inspiration__list
						li.inspiration__item
							.inspiration__item-content
								p.inspiration__item-text Design and technical drawings
								p.inspiration__num 80%
							.inspiration__line(style="width: 80%")
						li.inspiration__item
							.inspiration__item-content
								p.inspiration__item-text Measurments
								p.inspiration__num 70%
							.inspiration__line(style="width: 70%")
						li.inspiration__item
							.inspiration__item-content
								p.inspiration__item-text Furniture functionality analysis
								p.inspiration__num 75%
							.inspiration__line(style="width: 75%")
						li.inspiration__item
							.inspiration__item-content
								p.inspiration__item-text Interior visualization
								p.inspiration__num 40%
							.inspiration__line(style="width: 40%")
